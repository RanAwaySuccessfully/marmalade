{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Go: Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}"
        },
        {
            "name": "Go: Debug GTK 4 Build",
            "type": "go",
            "request": "launch",
            "mode": "exec",
            "program": "marmalade-gtk4",
            "preLaunchTask": "goWithGtk4"
        },
        {
            "name": "Go: Debug GTK 3 Build",
            "type": "go",
            "request": "launch",
            "mode": "exec",
            "program": "marmalade-gtk3",
            "preLaunchTask": "goWithGtk3"
        },
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "cwd": "${fileDirname}",
            "console": "integratedTerminal"
        }
    ]
}